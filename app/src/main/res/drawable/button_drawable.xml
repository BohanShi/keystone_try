<?xml version="1.0" encoding="utf-8"?>
<!--Create a photo that with smooth edge and use it as background photo for the button-->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <!--Set the Background Color-->
    <solid android:color="#F7DC6F " />
    <!--Set the photo edges/corner to round instead of square-->
    <corners android:radius="105dip" />
    <padding
        android:bottom="12dp"
        android:left="63dp"
        android:right="63dp"
        android:top="12dp"
        />
    <!--Sets the width and color of the border line-->

</shape>
